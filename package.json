{
  "name": "OpenGroup",
  "version": "0.0.1",
  "description": "OpenGroup.io",
  "main": "dist/index.html",
  "homepage": "http://opengroup.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opengroupio/es6.git"
  },
  "bugs": "https://github.com/opengroupio/es6/issues",
  "author": {
    "name": "Daniel Beeke"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss",
    "postinstall": "jspm install"
  },
  "keywords": [
    "opengroup"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "browser-sync": "^2.15.0",
    "del": "^2.2.0",
    "esdoc": "^0.4.4",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-cssnano": "^2.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jspm": "^0.5.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.2.0",
    "gulp-sass-lint": "^1.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^4.2.0",
    "jspm": "^0.16.27",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "sassdoc": "^2.1.20"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    }
  }
}
