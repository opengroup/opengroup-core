{
  "name": "OpenGroup",
  "version": "0.0.1",
  "description": "OpenGroup.io",
  "main": "OpenGroup.js",
  "homepage": "http://opengroup.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opengroupio/es6.git"
  },
  "bugs": "https://github.com/opengroupio/es6/issues",
  "author": {
    "name": "Daniel Beeke"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss",
    "postinstall": "jspm install"
  },
  "keywords": [
    "opengroup"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "browser-sync": "^2.15.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "del": "^2.2.0",
    "esdoc": "^0.4.4",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-cssnano": "^2.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jspm": "^0.5.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.2.0",
    "gulp-sass-glob": "^1.0.6",
    "gulp-sass-lint": "^1.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^4.2.0",
    "jspm": "^0.17.0-beta.29",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-growler-reporter": "https://github.com/danielbeeke/karma-growler-reporter#master",
    "karma-jspm": "^2.2.0 ",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "mocha": "^3.0.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "sassdoc": "^2.1.20"
  },
  "jspm": {
    "registry": "npm",
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.8",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.5.8",
      "angular-ui-router": "github:angular-ui/angular-ui-router-bower@^0.3.1",
      "clipboard": "^1.5.12",
      "danielbeeke/rhythmmeister": "github:danielbeeke/rhythmmeister@0.2.0",
      "jquery": "^3.1.1",
      "systemjs/plugin-text": "github:systemjs/plugin-text@^0.0.9"
    },
    "devDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "babel": "babel-core@^5.8.24",
      "babel-plugin-transform-decorators-legacy": "^1.3.4",
      "babel-runtime": "^5.8.24",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "core-js": "^1.1.4",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "plugin-babel": "systemjs-plugin-babel@^0.0.15",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "peerDependencies": {
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    },
    "overrides": {
      "github:angular-ui/angular-ui-router-bower@0.3.1": {
        "dependencies": {
          "angular": "*"
        },
        "meta": {
          "release/angular-ui-router.js.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-sanitize@1.5.8": {
        "main": "angular-sanitize",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-sanitize.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular@1.5.8": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.1": {
        "format": "amd"
      },
      "npm:lodash@4.16.2": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  }
}
